<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Clicker.Style">
    <Color x:Key="Vermillion">#D33F49</Color>
    <Color x:Key="Thistle">#D7C0D0</Color>
    <Color x:Key="Beige">#EFF0D1</Color>
    <Color x:Key="OceanGreen">#77BA99</Color>
    <Color x:Key="RaisinBlack">#262730</Color>

    <SolidColorBrush x:Key="VermillionBrush" Color="{StaticResource Vermillion}"></SolidColorBrush>
    <SolidColorBrush x:Key="ThistleBrush" Color="{StaticResource Thistle}"></SolidColorBrush>
    <SolidColorBrush x:Key="BeigeBrush" Color="{StaticResource Beige}"></SolidColorBrush>
    <SolidColorBrush x:Key="OceanGreenBrush" Color="{StaticResource OceanGreen}"></SolidColorBrush>
    <SolidColorBrush x:Key="RaisinBlackBrush" Color="{StaticResource RaisinBlack}"></SolidColorBrush>
</ResourceDictionary>